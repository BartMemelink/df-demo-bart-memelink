{
	"name": "working_with_apis",
	"properties": {
		"activities": [
			{
				"name": "rijksData",
				"type": "WebActivity",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"url": {
						"value": "@concat('https://www.rijksmuseum.nl/api/nl/collection?key=', variables('api-key'))",
						"type": "Expression"
					},
					"method": "GET"
				}
			}
		],
		"variables": {
			"api-key": {
				"type": "String",
				"defaultValue": "6epvixwD"
			}
		},
		"annotations": []
	}
}